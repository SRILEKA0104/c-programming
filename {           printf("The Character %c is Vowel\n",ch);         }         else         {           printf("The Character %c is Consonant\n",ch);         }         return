#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
    int main()
    {
        char ch;
        scanf("%c",&ch);
        if(ch=='a'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='I'||ch=='o'||ch=='u')
        {
          printf("The Character %c is Vowel\n",ch);
        }
        else
        {
          printf("The Character %c is Consonant\n",ch);
        }
        return 0;
    }
